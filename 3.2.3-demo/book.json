{
    "language": "zh-hans",
    "title": "demo",
    "structure": {
        "readme": "INTRODUCTION.md"
    },
    "links" : {
        "sidebar" : {
            "PDF版下载" : "/book.pdf"
        }
    },
    "plugins": [
        "-sharing",
        "-lunr", 
        "-search", 
        "search-pro@2.0.2",
        "splitter@0.0.8",
        "editlink@1.0.2",
        "expandable-chapters-small@0.1.7",
        "page-footer-ex@0.1.2",
        "sequence@0.1.1",
        "flow@1.1.7",
        "nomnoml@1.0.0",
        "echarts@0.0.17",
        "mermaid-cli@1.0.14"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "editlink": {
            "base": "https://github.com/helphi/Dockerfile-gitbook/edit/master/3.2.3-demo",
            "label": "编辑本页",
            "multilingual": false
        },
        "page-footer-ex": {
            "copyright": "&copy 版权所有",
            "markdown": false,
            "update_label": "<b>更新时间：</b>",
            "update_format": "YYYY-MM-DD HH:mm:ss"
        },
        "mermaid-cli": {
           "chromeArgs": ["--no-sandbox"]
        }
    },
    "pdf": {
        "fontSize": 14,
        "fontFamily": "Microsoft YaHei"
    }
}
